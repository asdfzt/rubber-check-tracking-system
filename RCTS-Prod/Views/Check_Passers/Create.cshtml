@model RCTS_Prod.Models.Check_Passer

@{
    ViewBag.Title = "Create Check Passer";
}

<h2>Create Check Passer</h2>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)
    <br />
    <fieldset>
        <legend>Check_Passer</legend>
        
        <div class="editor-label">
            @*@Html.LabelFor(model => model.Check_PasserID)*@
            <b>Drivers License</b>
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Check_PasserID)
            @Html.ValidationMessageFor(model => model.Check_PasserID)
        </div>
        <div class="editor-label">
            @*@Html.LabelFor(model => model.Last_Name)*@
            <b>Last Name</b>
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Last_Name)
            @Html.ValidationMessageFor(model => model.Last_Name)
        </div>

        <div class="editor-label">
            @*@Html.LabelFor(model => model.First_Name)*@
            <b>First Name</b>
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.First_Name)
            @Html.ValidationMessageFor(model => model.First_Name)
        </div>

        <div class="editor-label">
            @*@Html.LabelFor(model => model.Street)*@
            <b>Street</b>
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Street)
            @Html.ValidationMessageFor(model => model.Street)
        </div>

        <div class="editor-label">
            @*@Html.LabelFor(model => model.City)*@
            <b>City</b>
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.City)
            @Html.ValidationMessageFor(model => model.City)
        </div>

        <div class="editor-label">
            @*@Html.LabelFor(model => model.State)*@
            <b>State</b>
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.State)
            @Html.ValidationMessageFor(model => model.State)
        </div>

        <div class="editor-label">
            @*@Html.LabelFor(model => model.Zip)*@
            <b>Zip</b>
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Zip)
            @Html.ValidationMessageFor(model => model.Zip)
        </div>

        <div class="editor-label">
            @*@Html.LabelFor(model => model.Phone_No)*@
            <b>Phone No</b>
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Phone_No)
            @Html.ValidationMessageFor(model => model.Phone_No)
        </div>

        <p>
            <input type="submit" value="Create" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to Checks", "Create", "Checks")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
