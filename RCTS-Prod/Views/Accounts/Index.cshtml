@model IEnumerable<RCTS_Prod.Models.Account>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create") &nbsp&nbsp
    @Html.ActionLink("Back to Checks", "Create", "Checks")
</p>
<table>
    <tr>
        <th>
            @*@Html.DisplayNameFor(model => model.Account_No)*@
            <b>Account Number</b>&nbsp&nbsp
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.Bank.Routing_No)*@
            <b>Routing Number</b>
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Account_No)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Bank.Routing_No)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Account_No }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Account_No })
        </td>
    </tr>
}

</table>
